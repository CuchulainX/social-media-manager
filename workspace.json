{
  "version": 2,
  "projects": {
    "core": {
      "root": "libs/core",
      "sourceRoot": "libs/core/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/core/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/core"],
          "options": {
            "jestConfig": "libs/core/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "sns-auth-api": {
      "root": "apps/sns-auth-api",
      "sourceRoot": "apps/sns-auth-api/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/sns-auth-api",
            "main": "apps/sns-auth-api/src/main.ts",
            "tsConfig": "apps/sns-auth-api/tsconfig.app.json",
            "assets": ["apps/sns-auth-api/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/sns-auth-api/src/environments/environment.ts",
                  "with": "apps/sns-auth-api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "sns-auth-api:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/sns-auth-api/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/sns-auth-api"],
          "options": {
            "jestConfig": "apps/sns-auth-api/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "sns-manager": {
      "root": "apps/sns-manager",
      "sourceRoot": "apps/sns-manager/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/sns-manager",
            "index": "apps/sns-manager/src/index.html",
            "baseHref": "/",
            "main": "apps/sns-manager/src/main.tsx",
            "polyfills": "apps/sns-manager/src/polyfills.ts",
            "tsConfig": "apps/sns-manager/tsconfig.app.json",
            "assets": [
              "apps/sns-manager/src/favicon.ico",
              "apps/sns-manager/src/assets"
            ],
            "styles": ["apps/sns-manager/src/styles.scss"],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/sns-manager/src/environments/environment.ts",
                  "with": "apps/sns-manager/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "sns-manager:build",
            "hmr": true
          },
          "configurations": {
            "production": {
              "buildTarget": "sns-manager:build:production",
              "hmr": false
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/sns-manager/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/sns-manager"],
          "options": {
            "jestConfig": "apps/sns-manager/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "sns-twitter": {
      "root": "libs/sns/twitter",
      "sourceRoot": "libs/sns/twitter/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/sns/twitter/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/sns/twitter"],
          "options": {
            "jestConfig": "libs/sns/twitter/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "defaultProject": "sns-manager",
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "scss",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "scss"
      },
      "library": {
        "style": "scss",
        "linter": "eslint"
      }
    }
  }
}
