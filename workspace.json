{
  "version": 2,
  "projects": {
    "core": {
      "root": "libs/core",
      "sourceRoot": "libs/core/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/core/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/core"],
          "options": {
            "jestConfig": "libs/core/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "sns-manager": {
      "root": "apps/sns-manager",
      "sourceRoot": "apps/sns-manager/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/sns-manager",
            "main": "apps/sns-manager/src/main.ts",
            "tsConfig": "apps/sns-manager/tsconfig.app.json",
            "assets": ["apps/sns-manager/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/sns-manager/src/environments/environment.ts",
                  "with": "apps/sns-manager/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "sns-manager:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/sns-manager/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/sns-manager"],
          "options": {
            "jestConfig": "apps/sns-manager/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "sns-twitter": {
      "root": "libs/sns/twitter",
      "sourceRoot": "libs/sns/twitter/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/sns/twitter/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/sns/twitter"],
          "options": {
            "jestConfig": "libs/sns/twitter/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "social-media-manager": {
      "root": "apps/social-media-manager",
      "sourceRoot": "apps/social-media-manager/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/social-media-manager",
            "index": "apps/social-media-manager/src/index.html",
            "baseHref": "/",
            "main": "apps/social-media-manager/src/main.tsx",
            "polyfills": "apps/social-media-manager/src/polyfills.ts",
            "tsConfig": "apps/social-media-manager/tsconfig.app.json",
            "assets": [
              "apps/social-media-manager/src/favicon.ico",
              "apps/social-media-manager/src/assets"
            ],
            "styles": ["apps/social-media-manager/src/styles.scss"],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/social-media-manager/src/environments/environment.ts",
                  "with": "apps/social-media-manager/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "social-media-manager:build",
            "hmr": true
          },
          "configurations": {
            "production": {
              "buildTarget": "social-media-manager:build:production",
              "hmr": false
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": [
              "apps/social-media-manager/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/social-media-manager"],
          "options": {
            "jestConfig": "apps/social-media-manager/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "social-media-manager-e2e": {
      "root": "apps/social-media-manager-e2e",
      "sourceRoot": "apps/social-media-manager-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/social-media-manager-e2e/cypress.json",
            "devServerTarget": "social-media-manager:serve",
            "tsConfig": "apps/social-media-manager-e2e/tsconfig.json"
          },
          "configurations": {
            "production": {
              "devServerTarget": "social-media-manager:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/social-media-manager-e2e/**/*.{js,ts}"]
          }
        }
      }
    }
  }
}
